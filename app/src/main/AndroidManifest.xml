<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hi_depok.hi_depok">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".Activity_Main.SplashScreen"
            android:noHistory="true"
            android:theme="@style/Background_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Background_main">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.sikepok"
            android:theme="@style/Background">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.kapok"
            android:theme="@style/Background">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.kadepok"
            android:theme="@style/Background">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.lapok"
            android:theme="@style/Background">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.ucok"
            android:theme="@style/Background">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity_Main.about"
            android:noHistory="true"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_Main.login"
            android:theme="@style/Background_main" />
        <activity
            android:name=".Activity_Main.setprofile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_Main.profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity_Main.changepass"
            android:noHistory="true" />
        <activity android:name=".Sikepok_Diagnosa.sikepok_diagnosa" />
        <activity android:name=".Sikepok_RS.sikepok_rs" />
        <activity
            android:name=".Activity_Main.fokopok"
            android:theme="@style/Background" />
        <activity android:name=".Ucok.ucok_content" />
        <activity android:name=".Ucok.UcokDetailActivity" />
        <activity android:name=".Lapok.lapok_content" />
        <activity android:name=".fokopok.fokopok_content" />
        <activity android:name=".Ucok.danus_activity" />
        <activity android:name=".Ucok.ucok_listusaha" />
        <activity android:name=".Ucok.hasil_calculate" />
        <activity android:name=".Ucok.detail_danus" />
        <activity
            android:name=".Ucok.detail_target"
            android:parentActivityName=".Ucok.danus_activity" />
        <activity android:name=".fokopok.message"></activity>
    </application>

</manifest>